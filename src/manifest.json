{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "author":"jiangZiLong",
  "commands": [
    {
      "name": "Markdown",
      "identifier": "markdown.my-command-identifier",
      "script": "./markdown.js",
      "shortcut": "command option j"
    },
    {
      "name": "插入当前时间",
      "identifier": "setUpdateTime.my-command-identifier",
      "script": "./setUpdateTime.js",
      "shortcut": "command shift t"
    },
    {
      "name": "设置线条样式",
      "identifier": "lineDrawer.my-command-identifier",
      "script": "./lineDrawer.js",
      "shortcut": "command option l"
    },
    {
      "name": "延长画板",
      "identifier": "artboardLengthen.my-command-identifier",
      "script": "./artboardLengthen.js",
      "shortcut": "command shift c"
    },
    {
      "name": "图层整理",
      "identifier": "layersArrange.my-command-identifier",
      "script": "./layersArrange.js",
      "shortcut": ""
    }
  ],
  "menu": {
    "title": "Editor Assistant",
    "items": [
      "markdown.my-command-identifier"
      ,"lineDrawer.my-command-identifier"
      ,"setUpdateTime.my-command-identifier"
      ,"artboardLengthen.my-command-identifier"
      ,"layersArrange.my-command-identifier"
    ]
  }
}